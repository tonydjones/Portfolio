<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tapalong">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".tapActivity"></activity>
        <activity android:name=".songActivity"></activity>
        <activity android:name=".difficultyActivity"></activity>
        <activity android:name=".recordActivity"></activity>
        <activity android:name=".playActivity"></activity>
        <activity android:name=".choiceActivity"></activity>
        <activity android:name=".resultsActivity"></activity>
        <activity android:name=".SetupActivity"></activity>
        <activity android:name=".CalibrateActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
</manifest>